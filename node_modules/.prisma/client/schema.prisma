// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Country {
  id              Int          @id @default(autoincrement())
  name            String       @unique @db.VarChar(255)
  Year            Int
  energy_cons     Int
  energy_cons_cap Int
  createdAt       DateTime     @default(now())
  population_id   Population[] @relation("country-id")
  population_name Population[] @relation("country-name")
}

model Population {
  id            Int     @id @default(autoincrement())
  country_id    Int
  country_name  String
  population    Int
  pop_per_sqkm  Int
  num_of_houses Int
  country       Country @relation(fields: [country_id], references: [id], name: "country-id")
  c_name        Country @relation(fields: [country_name], references: [name], name: "country-name")
}
